<controls:OverwatchTextInputWindow
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._RMC14.Overwatch"
    Title="Повідомлення Загону" MinSize="480 104">
    <BoxContainer Orientation="Vertical">
        <LineEdit Name="MessageBox" Access="Public" />
        <BoxContainer Orientation="Horizontal">
            <Button Name="OkButton" Access="Public" Text="Підтвердити" HorizontalExpand="True" StyleClasses="OpenBoth" />
            <Button Name="CancelButton" Access="Public" Text="Відмінити" HorizontalExpand="True" StyleClasses="OpenBoth" />
        </BoxContainer>
    </BoxContainer>
</controls:OverwatchTextInputWindow>
